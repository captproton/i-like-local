<h1>Listing user_kinds</h1>

<table>
  <tr>
    <th>Title</th>
  </tr>
  
<% for user_kind in @user_kinds %>
  <tr>
    <td><%=h user_kind.title %></td>
    <td><%= link_to 'Show', user_kind_path(user_kind) %></td>
    <td><%= link_to 'Edit', edit_user_kind_path(user_kind) %></td>
    <td><%= link_to 'Destroy', user_kind_path(user_kind), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New user_kind', new_user_kind_path %>